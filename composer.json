{
    "name": "vrkansagara/laraapi",
    "description": "The Laravel Framework As API Layer",
    "keywords": ["framework", "laravel", "API"],
    "license": "BSD 3-Clause License",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arcanedev/laravel-settings": "^3.0",
        "barryvdh/laravel-cors": "^0.11.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "baum/baum": "^1.1",
        "creativeorange/gravatar": "~1.0",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/cashier": "~7.0",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "league/flysystem": "^1.0",
        "league/fractal": "^0.17.0",
        "predis/predis": "^1.1",
        "prettus/l5-repository": "^2.6",
        "roave/security-advisories": "dev-master",
        "spatie/laravel-permission": "^2.15",
        "tylercd100/lern": "^4.5",
        "vlucas/phpdotenv": "~2.2",
        "zendframework/zend-coding-standard": "^1.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "barryvdh/laravel-debugbar": "^3.1"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "serve": "php -S 0.0.0.0:8090 -d public/index.php -t public",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "lclear" :"php artisan cache:clear && php artisan cache:forget spatie.permission.cache && php artisan view:clear && rm -rf storage/debugbar/*"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
